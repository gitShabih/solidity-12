pragma solidity ^0.8.7;

contract simple {
    int a; 
    uint b;
    bool c;
    address d;

    function setA(int _a) public {
        a = _a;
    }
    function setB(uint _b) public {
        b = _b;
    }
    function setC(bool _c) public {
        c = _c;
    }
    function setD(address _d) public {
        d = _d;
    }
    function getA() public view returns(int) {
        return a;
    }
    function getB() public view returns(uint) {
        return b;
    }
    function getC() public view returns(bool) {
        return c;

    }
    function getD() public view returns(address) {
        return d;
    }
}
